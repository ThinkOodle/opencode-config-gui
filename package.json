{
  "name": "oodle-ai",
  "version": "1.0.13",
  "description": "OpenCode configuration app for macOS",
  "main": "./out/main/index.js",
  "type": "module",
  "author": "Oodle",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "package": "electron-vite build && electron-builder --mac",
    "package:mac": "electron-vite build && electron-builder --mac --arm64 --x64",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "typecheck:main": "tsc --noEmit -p electron/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p src/tsconfig.json",
    "release": "electron-vite build && electron-builder --mac --arm64 --x64 --publish always",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-updater": "^6.3.9",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "lucide-react": "^0.474.0",
    "postcss": "^8.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
}
